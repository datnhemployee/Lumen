{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es2017"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "utils/*": ["src/utils/*"]
    }
  }
  // "exclude": ["node_modules"],
  // "include": ["babel.config.js", "metro.config.js", "jest.config.ts"]
}
